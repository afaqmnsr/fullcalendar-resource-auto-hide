{
  "name": "fullcalendar-resource-auto-hide",
  "version": "1.0.2",
  "type": "module",
  "title": "FullCalendar Resource Auto-Hide Plugin",
  "description": "Automatically hide resources without events in the visible date range",
  "keywords": [
    "fullcalendar",
    "calendar",
    "resource",
    "timeline",
    "scheduler",
    "auto-hide",
    "visibility",
    "filter"
  ],
  "homepage": "https://github.com/afaqmnsr/fullcalendar-resource-auto-hide#readme",
  "bugs": "https://github.com/afaqmnsr/fullcalendar-resource-auto-hide/issues",
  "license": "MIT",
  "author": "Afaq Mansoor Khan <afaqmnsr0@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afaqmnsr/fullcalendar-resource-auto-hide.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.min.js",
  "jsdelivr": "dist/index.global.min.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "@fullcalendar/core": "^6.0.0",
    "@fullcalendar/resource-timeline": "^6.0.0"
  },
  "devDependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/resource-timeline": "^6.1.15",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.12.0",
    "tslib": "^2.8.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
    "prepublishOnly": "npm run build"
  }
}
